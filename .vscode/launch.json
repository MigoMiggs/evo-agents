    "version": "0.2.0",
    "configurations": [
        {
            "name": "Agent Evo Concierge",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "agent-evo-concierge.app.main:app",
                "--reload",
                "--host",
                "0.0.0.0",
                "--port",
                "8000"
            ],
            "jinja": true,
            "justMyCode": true,
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/agent-evo-concierge"
            }
        }
    ]
}